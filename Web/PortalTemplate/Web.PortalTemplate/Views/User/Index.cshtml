@model IEnumerable<Web.PortalTemplate.Models.UserModel>

@{
  ViewBag.Title = "All Users";
  Layout = "~/Views/Shared/_Layout.cshtml";
}

<h3>Following table shows Users list</h3>
<p>You can create a new user: <a href="~/User/Create" class="btn btn-default">Create user</a></p>

<table class="table table-hover">
  <thead class="thead-inverse">
    <tr>
      <th>Id</th>
      <th>Name</th>
      <th>Surename</th>
    </tr>
  </thead>
  <tbody>
    @if (Model != null && Model.Count() > 0)
    {
      foreach (var user in Model)
      {
        <tr>
          <td>@user.Id</td>
          <td>@user.Name</td>
          <td>@user.Surename</td>
          @*<td>
              <a href="~/User/Update/@user.Id" disabled>Update</a>
              <a href="~/User/Delete/@user.Id">Delete</a>
            </td>*@
        </tr>
      }
    }
  </tbody>
</table>